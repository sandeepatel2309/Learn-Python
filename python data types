
#String data type
pystring = "This is a string"

#Integer data type
pyint = 1991

#Float data type
pyfloat = 91.88

#Boolean data type
pyboolean = True

#List data type
pylist = ["List", 991, True, 12.22]

#Tuple data type
pytuple = ("Tuple", 119, False, 22.12)

#Set data type (will remove duplicate values)
setlist = [22, 34, 43, 34, 55, 11, 22, 33, "Mona", "Mona"]
set(setlist)
print("List values after set = ",setlist)

#Dictionary data type
mydictionary = {"Name": "Sam"}

#get a type of variable
print(type(pystring)," = ", pystring)
print(type(pyint)," = ", pyint)
print(type(pyfloat)," = ", pyfloat)
print(type(pyboolean)," = ", pyboolean)
print(type(pylist)," = ", pylist)
print(type(pytuple)," = ", pytuple)

#Print list, tuple in ascending/descending order order
ordlist = [23, 14, 34, 22, 65, 54]
ordlist.sort()
print("Ascending Order = ", ordlist)
ordlist.sort(reverse=True)
print("Descending Order = ", ordlist)

#Playing with List
indxlist = ["Hina", "Jaya", "Sushma", "Nirma", 91, 92]

#Find index and using that index print value on that index
myindx = indxlist.index("Nirma")
print("This value (", indxlist[myindx], ") available on ",myindx, " index")

#Manipulating List

indxlist.insert(1, "Rekha")
indxlist.remove(91)
indxlist.pop(5)
indxlist.append("Seema")
indxlist.extend("Ch")
print("Updated indxlist list = ", indxlist)
print("length of indxlist list = ", len(indxlist))
